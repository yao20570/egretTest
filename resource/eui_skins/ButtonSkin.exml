<?xml version='1.0' encoding='utf-8'?>
<e:Skin class="skins.ButtonSkin" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing" states="up,down,disabled">
	<e:Image id="_mask" width="100%" height="100%" source="{bgStr}" horizontalCenter="0" verticalCenter="0" scale9Grid="60,25,14,7" width.down="90%" height.down="90%" visible.down="false"/>
	<e:Image id="_bgImg" horizontalCenter="0" verticalCenter="0" width.up="100%" height.up="100%" width.down="90%" height.down="90%"
	         width="100%" height="100%"  source="{bgStr}" scale9Grid="60,25,14,7" source.disabled="{bgStr}" />
	<e:Image id="iconDisplay" scaleY.down="0.9" scaleX.down="0.9" horizontalCenter="0" verticalCenter="0"  source.disabled="{}"/>
	<e:Label text="{label}" fontFamily="黑体" horizontalCenter="0" verticalCenter="0" scaleX.down="0.9" scaleY.down="0.9"  size="22" textColor="0xffffff"/>
	<e:Rect id="_rect" width="100%" height="100%" fillAlpha="0.4" includeIn="down"/>
</e:Skin>